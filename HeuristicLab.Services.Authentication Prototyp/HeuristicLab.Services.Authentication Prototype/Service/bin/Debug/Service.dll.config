      <serviceBehaviors>
        <behavior name="HttpServiceBehavior">
          <serviceMetadata httpGetEnabled="true" httpGetUrl="http://127.0.0.1:8888/metadata" />
          <serviceDebug httpHelpPageUrl="http://127.0.0.1:8888/debug" />
