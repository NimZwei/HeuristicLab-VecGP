      </serviceBehaviors>
    </behaviors>
    <services>
      <service behaviorConfiguration="HttpServiceBehavior" name="Service.Services.Administration.AuthorizationManagementService">
        <endpoint address="http://127.0.0.1:8888/AuthorizationManagementEndpoint"
          binding="basicHttpBinding" bindingConfiguration="" contract="Service.Services.Administration.IAuthorizationManagementService" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/AuthorizationManagement"/>
          </baseAddresses>
        </host>
